{
    "tags": {
      "allowUnknownTags": true
    },
    "source": {
      "include": ["./src"]
    },
    "plugins": ["plugins/markdown"],    
    "opts": {
        "encoding": "utf8",
        "destination": "docs/",
        "recurse": true,
        "verbose": true,
        "readme": "../README.md"
    },
    "templates": {
        "better-docs": {
          "name": "Hrnet doc"
        }
      }
  }